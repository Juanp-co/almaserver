{
  "name": "almaserver",
  "version": "0.0.49",
  "description": "API to ALMA App.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/index.js ",
    "dev": "NODE_ENV=development nodemon",
    "build": "npm run lint && tsc -p ./ && npm run clean",
    "precommit": "npm run build && npm run docs",
    "lint": "eslint src/**/*.ts --quiet --fix --ignore-pattern \"node_modules/\" && tsc --noEmit",
    "docs": "apidoc -i ./Docs -o public/apidoc/",
    "clean": "rimraf dist",
    "migrations": "npx ts-node Migrations/DevMigration.ts",
    "migrations-prod": "NODE_ENV=production npx ts-node Migrations/DevMigration.ts",
    "fix-users-roles": "NODE_ENV=development npx ts-node Scripts/FixUsersRoles.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Juanp-co/almaserver.git"
  },
  "keywords": [
    "alma-backend"
  ],
  "author": "velantcode",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.968.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.17",
    "rimraf": "^3.0.2",
    "slug": "^4.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/mongoose": "^5.10.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.25",
    "@types/nodemailer": "^6.4.0",
    "@types/slug": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "apidoc": "^0.26.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.1.3"
  },
  "optionalDependencies": {
    "win-node-env": "^0.4.0"
  }
}
