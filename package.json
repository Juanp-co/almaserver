{
  "name": "alma_backend",
  "version": "0.0.2",
  "description": "API to ALMA App.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/index.js ",
    "dev": "npm run lint && NODE_ENV=development nodemon ./src/index.ts",
    "build": "npm run clean && npm run lint && tsc -p ./",
    "precommit": "npm run lint && npm run docs && npm run clean && tsc -p ./",
    "lint": "tsc --noEmit && eslint src/**/*.ts --quiet --fix --ignore-pattern \"node_modules/\"",
    "docs": "apidoc -i ./Docs -o public/apidoc/",
    "clean": "rimraf dist",
    "migrations": "npx ts-node Migrations/DevMigration2.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "alma-backend"
  ],
  "author": "velantcode",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "mongoose": "^5.11.5",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.16",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.10",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.0",
    "apidoc": "^0.25.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "optionalDependencies": {
    "win-node-env": "^0.4.0"
  }
}
